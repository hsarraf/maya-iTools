global string $selected_g;global int $fileId_g;global string $expName = "hi_record_Exp";proc hi_initialFunc () {  global string $selected_g;  global int $fileId_g;  global string $expName;  string $selectionList[] = `ls -sl`;  $selected_g = $selectionList[0];  string $recordPath = `textFieldGrp -q -tx hi_recordPathControl`;  string $recordPathHI = $recordPath + ".hi";  $fileId_g = `fopen $recordPathHI "w"`;  if (`objExists $expName`)	delete $expName;  expression -s ("hi_recordFunc")  -o "" -ae 1 -uc all -n $expName;  play -rec;}proc hi_recordFunc () {  global string $selected_g;  global int $fileId_g;  vector $pos = `getAttr ($selected_g + ".t")`;  fprint $fileId_g ($pos + "\n");}proc hi_closeFileFunc () {  global int $fileId_g;  fclose $fileId_g;}proc hi_runFunc() {  string $selectionList[] = `ls -sl`;  string $selected = $selectionList[0];//  global string $selected_g;  global string $expName;  if (`objExists $expName`)	delete $expName;  string $dataPath = `fileDialog -dm "*.hi"`;  int $fId = `fopen $dataPath "r"`;  int $i = 1;  while (!`feof $fId`) {	currentTime $i;	vector $pos = `fgetline $fId`;	xform -ws -t ($pos.x) ($pos.y) ($pos.z) $selected;	setKeyframe -at translate $selected;	$i++;  }  fclose $fId ;}proc hi_resetFunc() {  string $selectionList[] = `ls -sl`;  string $selected = $selectionList[0];  delete ($selected + "_translateX");  delete ($selected + "_translateY");  delete ($selected + "_translateZ");}if (`window -ex hi_recordBoardWin`)  deleteUI hi_recordBoardWin;window -t "Record Board" hi_recordBoardWin; columnLayout -adj 1;	textFieldGrp -l "Record Path" -adj 1 -cal 1 left hi_recordPathControl;	button -l "Record" -c hi_initialFunc;	button -l "Complete" -c hi_closeFileFunc;	separator -h 20;	button -l "Run" -c hi_runFunc;	button -l "Reset" -c hi_resetFunc;showWindow hi_recordBoardWin;